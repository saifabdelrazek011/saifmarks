{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  },
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1",
    "^template/(.*)$": "<rootDir>/template/$1",
    "^@/(.*)$": "<rootDir>/$1",
    "^config$": "<rootDir>/src/config"
  },
  "setupFiles": ["<rootDir>/test/setupEnv.ts"]
}
